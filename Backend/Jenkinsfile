pipeline {
    agent any
    stages {
        stage ("Clone Git") {
            steps {
                git branch: 'main', url: 'https://github.com/nisharathod231/Kalopsia.git'
            }
        }
        stage ("Build via Maven") {
            steps {
                sh 'mvn -f Backend/pom.xml clean install'
            }
        }
    }
}
